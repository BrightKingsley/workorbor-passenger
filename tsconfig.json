{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "types": [
      "node"
    ],
    "paths": {
      "#/*": [
        "./src/*"
      ],
      "lib/*": [
        "./src/lib/*"
      ],
      "platform/*": [
        "./src/platform/*"
      ],
      "state/*": [
        "./src/state/*"
      ],
      "view/*": [
        "./src/view/*"
      ]
    }
  }
}